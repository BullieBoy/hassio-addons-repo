{
  "name": "Music Assistant",
  "version": "0.0.14",
  "description": "Media library manager for (streaming) media",
  "url": "https://github.com/marcelveldt/musicassistant",
  "slug": "music_assistant",
  "startup": "application",
  "boot": "auto",
  "arch": ["amd64" ],
  "map": ["share:rw","ssl","config:rw"],
  "webui": "http://[HOST]:[PORT:8095]",
  "tmpfs": "size=64m",
  "host_network": true,
  "environment": {},
  "image": "marcelveldt/musicassistant",
  "homeassistant_api": true,
  "ingress": true,
  "ingress_port": 0,
  "ingress_entry": "musicassistant",
  "options": {
    "debug_messages": false,
    "data_dir": "/config/mass_data",
    "use_nightly": false
  },
  "schema": {
    "debug_messages": "bool",
    "data_dir": "str",
    "use_nightly": "bool"
  },
  "ports": {
    "8095": 8095,
    "8096": 8096
  },
  "ports_description": {
    "8095": "HTTP port",
    "8096": "HTTPS port"
  }
}
